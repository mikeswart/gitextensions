<Window x:Class="GitSlave.Internal.BranchPromptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="BranchPromptWindow" Height="161" Width="420">
    <Grid RenderTransformOrigin="0.495,0.623">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="44"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1" Background="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}">
            <Button Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Center" Width="75" Height="20" Margin="248,14,0,10" IsCancel="True"/>
            <Button x:Name="OkButton" Click="OkButton_OnClick" Content="Ok" HorizontalAlignment="Left" VerticalAlignment="Center" Width="75" Margin="328,14,0,10" Height="20" IsDefault="True"/>
        </Grid>
    </Grid>
</Window>
